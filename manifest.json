{
  "manifest_version": 3,
  "name": "9:14 Popup Extension",
  "version": "1.0",
  "description": "A Chrome extension that shows a popup with a 9:14 ratio.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  }
}
